function add(number1, number2) {
  var result;
  console.log("number1 is ",number1);
  console.log("number2 is ",number2);
  console.log("result before addition is ",result);
  result = number1 + number2;
  console.log("result after addition is ",result);
  return result;
}


function subtration(number1,number2){
  var result;
  console.log("number1 is ",number1);
  console.log("number2 is ",number2)
  console.log("result before subtration is ", result);
  result = number1 - number2;
  console.log("result after subtration is ", result);
  return result
}


function multiple(number1, number2) {
  var result;
  console.log("number1 is ",number1);
  console.log("number2 is ",number2);
  console.log("result before multiple is ", result);
  result = number1 * number2;
  console.log("result after multiple is", result)
  return result;
}

